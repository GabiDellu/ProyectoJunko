<%- include('../partials/header') %>
<main>
    <div> 
      <h1 class="tituloh1">CREA TU CUENTA</h1>
      <h2  class="tituloh2">Completa el formulario para ser parte del mundo de los Funkos</h2>
    </div>
    <form class="formulario">
        <label class="formulario__label"  for="fname">Nombre:</label>
        <input class="formulario__input" type="text" id="fname" name="fname">
        
        <label class="formulario__label" for="lname">Apellido:</label>
        <input class="formulario__input" type="text" id="lname" name="lname"> 
        
        <label class="formulario__label" for="email">Email:</label>
        <input class="formulario__input" type="email" id="email" name="email">
        
        <label class="formulario__label" for="pwd">Contraseña:</label>
        <input class="formulario__input" type="password" id="pwd" name="pwd">
        
        <label class="formulario__label" for="rpwd">Repita contraseña:</label>
        <input class="formulario__input" type="password" id="rpwd" name="rpwd" placeholder="">
    </form>
    
    <div class="formulario__checkbox">
        <button class="formulario__boton" type="reset" value="Registrar" onclick="">Registrar</button>
        <input type="checkbox" id="acepto" name="acepto" value="Acepto">
        <label class="formulario__label" for="acepto">Acepto</label>
        <a class="formulario__label-a" href="">Términos y condiciones</a>
    </div>      
  </main>
  <%- include('../partials/footer') %>
  <script src="https://cdn.jsdelivr.net/npm/iconify-icon@1.0.8/dist/iconify-icon.min.js"></script>
</body>
</html>